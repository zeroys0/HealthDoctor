                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:id="@+id/tv_phone_time"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/x120"
                android:gravity="right"
                android:text=""
                android:textColor="@color/text_grey"
                />
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/arrow_right_grey"
                android:layout_marginRight="@dimen/x35"
                />
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/y2"
                android:background="@color/bg"
                android:layout_marginLeft="@dimen/x36"
                android:layout_marginRight="@dimen/x36"
                android:layout_alignParentBottom="true"/>
        </RelativeLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/y36"
        android:background="@drawable/bg_white_radius"
        android:layout_marginLeft="@dimen/x30"
        android:orientation="vertical"
        android:layout_marginRight="@dimen/x30"
        >
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/y141"
            >
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/img_home_cure"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/x38"
                />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/x42"
                android:textColor="@color/text_black"
                android:layout_marginLeft="@dimen/x155"
                android:layout_centerVertical="true"
